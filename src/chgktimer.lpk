    <Version Release="1"/>
    <Files>
      <Item>
        <Filename Value="timerframe.pas"/>
        <UnitName Value="TimerFrame"/>
      </Item>
    </Files>
    <RequiredPkgs>
      <Item>
        <PackageName Value="playwavepackage"/>
      </Item>
      <Item>
        <PackageName Value="LCLBase"/>
      </Item>
      <Item>
        <PackageName Value="FCL"/>
      </Item>
